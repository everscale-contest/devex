{
  "name": "ton-solidity",
  "displayName": "ton-solidity",
  "description": "TON Solidity Language for Visual Studio Code",
  "version": "0.0.1",
  "keywords": [
    "solidity",
    "ton-solidity",
    "TON",
    "FreeTON",
    "Free TON",
    "blockchain",
    "tondev",
    "highlighting"  
  ],  
  "publisher": "freeton20",
  "license": "MIT",
  "engines": {
    "vscode": "^1.59.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freeton20/ton-highlight"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:ton-solidity"
  ],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "ton-solidity",
        "aliases": [
          "ton-solidity"
        ],
        "extensions": [
          ".sol"
        ],
        "configuration": "./ton-solidity.configuration.json"
      }
    ],
    "snippets": [
      {
        "language": "ton-solidity",
        "path": "./snippets/snippets.json"
      }
    ],
    "grammars": [
      {
        "language": "ton-solidity",
        "scopeName": "source.ton-solidity",
        "path": "./syntaxes/ton-solidity.json"
      }
    ]
  },
  "icon": "assets/icon.png",
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "tondev": "^0.9.0"
  }
}
